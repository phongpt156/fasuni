  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "linebreak-style": "off",
      "sort-imports": "off",
      "no-console": "off",
      "no-extra-semi": "off",
      "arrow-parens": "off",
      "extensions": "off",
      "default-case": "off",
      "max-len": "off",
      "import/extensions": "off",
      "no-param-reassign": "off",
      "prefer-default-export": "off",
      "no-alert": "off",
      "prefer-destructuring": "off",
      "consistent-return": "off",
      "array-callback-return": "off",
      "no-restricted-syntax": "off",
      "arrow-body-style": "off",
      "no-shadow": "off",
      "no-underscore-dangle": "off",
      "no-lonely-if": "off",
      "import/prefer-default-export": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
